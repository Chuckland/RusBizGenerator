for (var i = 0, l = [selection count]; i < l; i++) {
	var layer = selection[i];
	var innFl = "";
	innFl += Math.floor(Math.random() * 9)+1;


	for (var j = 0; j < 9; j++) {
		var randomDigit = Math.floor(Math.random() * 10);
		innFl += randomDigit;
	}

	[layer setStringValue: innFl];
	[layer setName: innFl];
}



